{
  "database": {
    "host": "prod-db.example.com",
    "port": 5432,
    "ssl": true,
    "name": "myapp_prod"
  },
  "api": {
    "baseUrl": "https://api.example.com",
    "timeout": 10000,
    "ssl": true
  },
  "debug": {
    "enabled": true,
    "level": "error"
  },
